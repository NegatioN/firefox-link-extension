{

  "manifest_version": 2,
  "name": "link-extension",
  "version": "1.0",

  "description": "Sends your current tab url to a preconfigured server",

  "icons": {
    "48": "icons/icon_48.png",
    "96": "icons/icon_96.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "browser_style": true,
    "default_icon": "icons/icon_48.png",
    "default_title": "link-extension",
    "default_popup": "popup/popup.html"
  }

}